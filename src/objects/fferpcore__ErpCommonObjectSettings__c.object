<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Contains the settings that determine the behavior of common objects or functionality affected by common objects.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>fferpcore__AllowBillingDocumentLineReparenting__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Indicates that you can reparent billing document line items.</description>
        <externalId>false</externalId>
        <inlineHelpText>Indicates that you can reparent billing document line items.</inlineHelpText>
        <label>Allow Billing Document Line Reparenting</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>fferpcore__DisableCreditTermsMergeValidation__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Indicates whether Foundations should prevent accounts from being merged if more than one of them has an account credit terms record.</description>
        <externalId>false</externalId>
        <inlineHelpText>Indicates that you do not want to prevent accounts from being merged when more than one of them has an account credit terms record. This might result in accounts with more than one account credit terms record.</inlineHelpText>
        <label>Disable Credit Terms Merge Validation</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>fferpcore__MaintainAccountValidAddressStatus__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>Indicates whether Foundations should maintain the Address Validated fields on Account</description>
        <externalId>false</externalId>
        <inlineHelpText>Indicates that you want to automatically update the Billing Address Validated and Shipping Address Validated fields on the Account object when an address is validated or edited.</inlineHelpText>
        <label>Update Address Validated Fields</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>FDN Common Object Settings</label>
    <visibility>Public</visibility>
</CustomObject>
