    <fields>
        <fullName>pse__Save_Column_Prefs__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>If selected, customizations to width, order, and visibility of columns in the Project Task Gantt will be remembered per user.</inlineHelpText>
        <label>Save Column Preferences</label>
        <trackTrending>false</trackTrending>
