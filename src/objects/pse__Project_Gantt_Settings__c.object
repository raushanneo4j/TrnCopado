        <fullName>pse__Project_Manager_Access__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Determines levels of Gantt edit access for the Project Manager. Values are &quot;Full&quot; (default) - can edit created projects, &quot;Read&quot;- has only read-only access, or &quot;None&quot; - access determined by Permission Controls.</inlineHelpText>
        <label>Project Manager Access</label>
        <length>15</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>pse__Project_Tooltip__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
