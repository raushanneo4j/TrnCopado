<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Settings that control aspects of Project Versioning and Baselines at a user or profile level.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>pse__Notifications_Chatter__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected, notifications from Project Version batch processes are sent via Chatter to recipients listed in the Notifications Recipients setting. This is deselected by default.</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected, notifications from Project Version batch processes are sent via Chatter to recipients listed in the Notifications Recipients setting. This is deselected by default.</inlineHelpText>
        <label>Notifications Chatter</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Notifications_Email__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected, notifications from Project Version batch processes are sent via email to recipients listed in the Notifications Recipients setting. This is deselected by default.</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected, notifications from Project Version batch processes are sent via email to recipients listed in the Notifications Recipients setting. This is deselected by default.</inlineHelpText>
        <label>Notifications Email</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Notifications_Recipients__c</fullName>
        <deprecated>false</deprecated>
        <description>Specifies the notification recipients for Project Version batch notifications sent via Chatter or email. The value is a list of email addresses separated by semicolons (;). This setting is null by default.</description>
        <externalId>false</externalId>
        <inlineHelpText>Specifies the notification recipients for Project Version batch notifications sent via Chatter or email. The value is a list of email addresses separated by semicolons (;). This setting is null by default.</inlineHelpText>
        <label>Notifications_Recipients</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>pse__Notifications_Task__c</fullName>
