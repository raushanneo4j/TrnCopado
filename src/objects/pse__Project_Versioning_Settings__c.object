        <deprecated>false</deprecated>
        <description>If selected, notifications from Project Version batch processes are sent via tasks. This is deselected by default.</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected, notifications from Project Version batch processes are sent via tasks. This is deselected by default.</inlineHelpText>
        <label>Notifications Task</label>
        <trackTrending>false</trackTrending>
