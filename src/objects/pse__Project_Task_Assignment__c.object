    <validationRules>
        <fullName>pse__Cannot_Associate_RR_On_PTA_Creation</fullName>
        <active>false</active>
        <description>Validates whether Resource Request can be associated with Project Task Assignment on creation.</description>
        <errorConditionFormula>AND(ISNEW() ,NOT( ISBLANK(pse__Resource_Request__c)))</errorConditionFormula>
        <errorMessage>A resource request will be created automatically with the new project task assignment. Please clear the Resource Request field and try again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Cannot_Specify_Hours_And_Percent</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK( pse__Allocation_Hours__c )),NOT(ISBLANK( pse__Allocation_Percentage__c )))</errorConditionFormula>
        <errorMessage>Cannot apply both Allocation Hours and Allocation Percentage</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Cannot_Specify_Res_And_Ext_Res</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(pse__Resource__c)), NOT(LEN(pse__External_Resource__c)=0))</errorConditionFormula>
        <errorMessage>Cannot specify both Resource and External Resource</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__PTA_RR_Project_Mismatch</fullName>
        <active>true</active>
        <description>Validates whether the Project Task Assignment and Resource Request belong to the same Project.</description>
        <errorConditionFormula>AND( NOT(ISBLANK( pse__Resource_Request__c )), pse__Project_Task__r.pse__Project__c  &lt;&gt;  pse__Resource_Request__r.pse__Project__c)</errorConditionFormula>
        <errorMessage>We cannot associate a project task assignment with the resource request because they do not belong to the same project. Enter a different resource request and try again.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Resc_OR_Ext_Resc_OR_Resc_Role_Required</fullName>
        <active>true</active>
        <description>Validation to check project task assignment cannot save without a Resource, External Resource or Resource Role.</description>
        <errorConditionFormula>AND(ISBLANK(pse__Resource__c),ISBLANK(pse__External_Resource__c),  ISBLANK(TEXT( pse__Resource_Role__c ))   )</errorConditionFormula>
        <errorMessage>We can&apos;t save a project task assignment without a Resource, External Resource or Resource Role.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Resource_OR_External_Resource_Required</fullName>
        <active>false</active>
        <description>[DEPRECATED]</description>
        <errorConditionFormula>AND(ISBLANK(pse__Resource__c),ISBLANK(pse__External_Resource__c))</errorConditionFormula>
        <errorMessage>Resource or External Resource is required</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
