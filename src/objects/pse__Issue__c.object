    <validationRules>
        <fullName>pse__Close_date_mandatory_if_status_is_closed</fullName>
        <active>true</active>
        <description>The Closed Date cannot be blank if the status is set to Completed or Closed - No Action.</description>
        <errorConditionFormula>(AND(OR(ISPICKVAL(pse__Status__c,&apos;Completed&apos;), ISPICKVAL(pse__Status__c,&apos;Closed - No action&apos;)), ISBLANK(pse__Closed_Date__c)))</errorConditionFormula>
        <errorMessage>The Closed Date cannot be blank if the status is set to Completed or Closed - No Action.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Closed_Date_Blank_If_Status_Not_Closed</fullName>
        <active>true</active>
        <description>The Closed date must be blank if the status is set to  Not Yet Started, In Progress or On Hold.</description>
        <errorConditionFormula>AND(OR( ISPICKVAL(pse__Status__c ,&apos;Not Yet Started&apos;),ISPICKVAL(pse__Status__c ,&apos;In Progress&apos;),ISPICKVAL(pse__Status__c ,&apos;On Hold&apos;)), (!ISBLANK(pse__Closed_Date__c ))
)</errorConditionFormula>
        <errorMessage>The Closed date must be blank if the status is set to  Not Yet Started, In Progress or On Hold.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>pse__Closed_Date_Greater_Than_Date_Raised</fullName>
        <active>true</active>
        <description>The issue&apos;s closed date must be after it was raised.</description>
        <errorConditionFormula>pse__Closed_Date__c&lt; pse__Date_Raised__c</errorConditionFormula>
        <errorMessage>The issue&apos;s closed date must be after it was raised.</errorMessage>
    </validationRules>
