<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Used by the Customer Success Team to map Customer Success Architects (CSA) to the Account territories to be able to populate the &apos;CSA&apos; field on the Customer Project Profile (CPP) with the name of the correct user. JIRA: SFDCSPT-449</description>
    <fields>
        <fullName>CSA__c</fullName>
        <businessOwnerGroup>Customer_Success</businessOwnerGroup>
        <businessStatus>Active</businessStatus>
        <description>Stores the ID of the Customer Success Architect (CSA) assigned to a particular global territory. JIRA: SFDCSPT-449</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>Stores the ID of the Customer Success Architect (CSA) assigned to a particular global territory.</inlineHelpText>
        <label>CSA</label>
        <length>255</length>
        <required>false</required>
        <securityClassification>Internal</securityClassification>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>GlobalRegion__c</fullName>
        <businessOwnerGroup>System_Admins</businessOwnerGroup>
        <businessStatus>Active</businessStatus>
        <description>List of the global business regions used at Neo4j. JIRA: SFDCSPT-449</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>List of the global business regions used at Neo4j.</inlineHelpText>
        <label>Global Region</label>
        <required>false</required>
        <securityClassification>Internal</securityClassification>
        <type>Picklist</type>
        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>APAC</fullName>
                    <default>true</default>
                    <label>APAC</label>
                </value>
                <value>
                    <fullName>EMEA</fullName>
                    <default>false</default>
                    <label>EMEA</label>
                </value>
                <value>
                    <fullName>LATAM</fullName>
                    <default>false</default>
                    <label>LATAM</label>
                </value>
                <value>
                    <fullName>NA</fullName>
                    <default>false</default>
                    <label>NA</label>
                </value>
                <value>
                    <fullName>None</fullName>
                    <default>false</default>
                    <label>None</label>
                </value>
                <value>
                    <fullName>Unassigned</fullName>
                    <default>false</default>
                    <label>Unassigned</label>
                </value>
            </valueSetDefinition>
        </valueSet>
    </fields>
    <label>CSA Global Territory Allocation</label>
    <pluralLabel>CSA Global Territory Allocations</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
