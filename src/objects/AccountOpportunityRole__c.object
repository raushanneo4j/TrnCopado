    <validationRules>
        <fullName>cloud_platform_is_req_if_onprem_is_no</fullName>
        <active>true</active>
        <description>If “On-premise” (On_premise_deployment__c) is No then require “cloud platform…” field</description>
        <errorConditionFormula>AND(
isblank(TEXT(Cloud_Platform_for_Project_Deployment__c )),
 ISPICKVAL(On_premise_deployment2__c, &quot;No&quot;)
)</errorConditionFormula>
        <errorDisplayField>Cloud_Platform_for_Project_Deployment__c</errorDisplayField>
        <errorMessage>Please input [Cloud Platform for Project Deployment].   (  [Cloud Platform for Project Deployment] is required when [On-premise deployment] is NO. )</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dep_cloud_platform_is_req_if_onprem_is_0</fullName>
        <active>false</active>
        <description>Can you make it so that if “On-premise” (On_premise_deployment__c) is checked that satisfies the mandatory “cloud platform…” field (Cloud_Platform_for_Project_Deployment__c)?</description>
        <errorConditionFormula>AND(
isblank( z_Cloud_Platform_for_Project_Deployment__c ),
NOT(On_premise_deployment__c)
)</errorConditionFormula>
        <errorMessage>Please input [Cloud Platform for Project Deployment].   (  [Cloud Platform for Project Deployment] is required when [On-premise deployment] is unchecked. )</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>req_other_detail_if_cloud_plat_is_other</fullName>
        <active>true</active>
        <description>a7z3s0000000NolAAE</description>
        <errorConditionFormula>if(and(
ISPICKVAL(  Cloud_Platform_for_Project_Deployment__c , &quot;other&quot;),
isblank( Other_Detail__c )
),true,false)</errorConditionFormula>
        <errorDisplayField>Other_Detail__c</errorDisplayField>
        <errorMessage>[Other Detail] is required when [Cloud Platform for Project Deployment] is &quot;other&quot;.</errorMessage>
    </validationRules>
