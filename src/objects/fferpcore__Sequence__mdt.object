<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Contains the numbering sequences that you can use for fields on your objects. You can specify the starting value of the sequence, a format made from static data, sequence parts, and leading zeros.</description>
    <fields>
        <fullName>fferpcore__Format__c</fullName>
        <deprecated>false</deprecated>
        <description>Format of the number sequence to be placed in the field. You can specify static data, the index of a sequence part enclosed in curly brackets, and leading zeros enclosed in curly brackets.</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>Format of the number sequence to be placed in the field. You can specify static data, the index of a sequence part enclosed in curly brackets, and leading zeros enclosed in curly brackets.</inlineHelpText>
        <label>Format</label>
        <length>255</length>
        <required>true</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>fferpcore__ResetSequenceForParts__c</fullName>
        <deprecated>false</deprecated>
        <description>A comma separated list of Sequence Part Indexes for which to reset to the Starting Value when the data returned by the Value field changes.</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>A comma separated list of Sequence Part Indexes for which to reset to the Starting Value when the data returned by the Value field changes.</inlineHelpText>
        <label>Reset Sequence for Parts</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>fferpcore__StartingValue__c</fullName>
        <deprecated>false</deprecated>
        <description>The number from which to start the sequence.</description>
        <externalId>false</externalId>
        <fieldManageability>SubscriberControlled</fieldManageability>
        <inlineHelpText>The number from which to start the sequence.</inlineHelpText>
        <label>Starting Value</label>
        <precision>18</precision>
        <required>true</required>
        <scale>0</scale>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <label>Sequence</label>
    <pluralLabel>Sequences</pluralLabel>
    <visibility>Public</visibility>
</CustomObject>
