<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>These preferences control individual customizations to the appearance of Project Task Gantt and its interactions with users.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>pse__Alert_Task_Dates_Milestone__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected, an alert graphic and tooltip message displays when Task dates are out of the Milestone target date range.</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected, an alert graphic and tooltip message displays when Task dates are out of the Milestone target date range.</inlineHelpText>
        <label>Alerts - Task dates - Milestone</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Alert_Task_Dates_PTA_Dates__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected, an alert graphic and tooltip message displays when Task dates are out of the Project Task Assignment dates range.</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected, an alert graphic and tooltip message displays when Task dates are out of the Project Task Assignment dates range.</inlineHelpText>
        <label>Alerts - Task dates - PTA dates</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Alert_Task_Dates_Project__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected, an alert graphic and tooltip displays when Task dates are out of the Project start date and end date ranges.</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected, an alert graphic and tooltip displays when Task dates are out of the Project start date and end date ranges.</inlineHelpText>
        <label>Alerts - Task dates - Project</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Alert_Task_Hours_PTA_Hours__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>If selected, an alert graphic and tooltip message displays when task hours are out the range of Project Task assignment hours.</description>
        <externalId>false</externalId>
        <inlineHelpText>If selected, an alert graphic and tooltip message displays when task hours are out the range of Project Task assignment hours.</inlineHelpText>
        <label>Alerts - Task hours - PTA hours</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Enable_Day_Detail__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>Allows the user to enable or disable Day Detail Zoom level.</description>
        <externalId>false</externalId>
        <inlineHelpText>When true, Project Task Gantt offers a Day Detail zoom level, showing the hours scheduled in an expanded day display. The default is false.</inlineHelpText>
        <label>Enable Day Detail</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Hide_Save_Preview__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Determines whether a dialog box is displayed every time you click Save. This allows you to review the proposed changes to project tasks before committing them to the actual records in your organization. The setting is deselected by default.</inlineHelpText>
        <label>Hide Preview and Confirmation on Save</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Resource_Demand_Default__c</fullName>
        <defaultValue>&quot;resource requests&quot;</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Specifies the default resource demand type when clicking the Create Resource Demand button in Project Task Gantt.  Values are: &quot;resource requests&quot;, &quot;held resource requests&quot;, or &quot;assignments&quot;. The default value is &quot;resource requests&quot;.</inlineHelpText>
        <label>Resource Demand Default</label>
        <length>75</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>pse__Show_Task_Notes__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Show Task Notes in Gantt</inlineHelpText>
        <label>Show Task Notes</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Single_Click_Task_Selection__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>This allows a user to change the default behavior of Gantt from double-click to single-click in the task pane.</description>
        <externalId>false</externalId>
        <inlineHelpText>Allows you to change default Project Task Gantt task pane selection behavior from double-click to single-click. The setting is deselected by default.</inlineHelpText>
        <label>Single Click Task Selection</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Suppress_Auto_Link_PTA__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <description>This allows a user to change the default behavior of Gantt so that new project task assignments will not be linked to existing assignment or resource request.</description>
        <externalId>false</externalId>
        <inlineHelpText>When selected, allows you to change the default behavior of Project Task Gantt so that new project task assignments are not linked to existing assignment or resource request. This setting is deselected by default.</inlineHelpText>
        <label>Suppress Auto Link Assignment</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Zoom_Level__c</fullName>
        <defaultValue>&quot;week&quot;</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Specifies the default zoom level for the timeline. Valid values are: quarter, month, week, day or daydetail. This setting is set to week by default.</inlineHelpText>
        <label>Zoom Level</label>
        <length>25</length>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <label>Project Task Gantt Preferences</label>
    <visibility>Public</visibility>
</CustomObject>
