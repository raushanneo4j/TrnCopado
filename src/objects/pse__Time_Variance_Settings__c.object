<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>Hierarchy</customSettingsType>
    <description>Defines the properties of the Time Variance calculation report when clicking the Generate Time Variance button from a region, practice, or group.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>pse__Allowed_Variance_Over__c</fullName>
        <defaultValue>0</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Specifies the time variance over scheduled working hours to include in time variance calculations.</inlineHelpText>
        <label>Allowed Variance Over</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>pse__Allowed_Variance_Under__c</fullName>
        <defaultValue>0</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Specifies the time variance under scheduled working hours to include in time variance calculations.</inlineHelpText>
        <label>Allowed Variance Under</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>pse__Exclude_Billable_Hours__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>If selected, billable hours are ignored when calculating Time Variance.</inlineHelpText>
        <label>Exclude Billable Hours</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Exclude_Credited_Hours__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>If selected, credited hours are ignored when calculating Time Variance.</inlineHelpText>
        <label>Exclude Credited Hours</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Exclude_Excluded_Hours__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>If selected, excluded hours are ignored when calculating Time Variance.</inlineHelpText>
        <label>Exclude Excluded Hours</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Report_All__c</fullName>
        <defaultValue>false</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Specifies that time variance calculations include details on all resources, including resources who do not show outside the allowed variance ranges.</inlineHelpText>
        <label>Report All</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>pse__Use_Percentage__c</fullName>
        <defaultValue>true</defaultValue>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>If true, the values in the Allowed Variance Over and Allowed Variance Under settings are percentages of time variance. If false, the values in the Allowed Variance Over and Allowed Variance Under settings are hours.  This setting is true by default.</inlineHelpText>
        <label>Use Percentage</label>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <label>Time Variance Settings</label>
    <visibility>Public</visibility>
</CustomObject>
