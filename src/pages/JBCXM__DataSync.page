<apex:page docType="html-5.0" controller="JBCXM.AdminBroker" sidebar="false" title="Data Sync" extensions="JBCXM.NSRemoting,JBCXM.GSCommonBroker" tabStyle="Administration__tab">
    <script src="{!JSINHTMLENCODE(GainsightCDNServerDomain)}gs-ui-static/{!JSINHTMLENCODE(GainsightAppVersion)}/GSVendor/js/vendor/HTMLSanitizer.js"></script>
	<script>
		var GS = GS || {};
		GS.userConfig = {
			userLocale : {!(dateTimeFormatByLocale)},
			userName : "{!(JSINHTMLENCODE($User.FirstName))}"+" "+"{!(JSINHTMLENCODE($User.LastName))}",
			userId : "{!$User.Id}",
			emailId : "{!$User.Email}",
            photo       : "{!(profilePhoto)}"
		};
		GS.currentOrgDatetime={!(currentOrgDatetime)};
		GS.packageNS            = "{!(pkgNS)}";
        GS.isPackagedInstance   = (GS.packageNS) ? "true" : "false";
		GS.remotingEndPoint     = '{!$RemoteAction.AdminBroker.adminRequestBroker}';
		GS.remotingBulkEndPoint     = '{!$RemoteAction.AdminBroker.adminRequestBroker}';
		GS.commonRemotingEndPoint =  '{!$RemoteAction.GSCommonBroker.gsCommonRequestBroker}';
		GS.remoting 			= "{!$RemoteAction.NSRemoting.apinsinvoke}";
		GS.sessionId 			= "{!NSSessionId}";
		GS.orgId 				= "{!$Organization.Id}";
		GS.nsParams             = fakeEval('{!JSINHTMLENCODE(nsParams)}');
		 //Start of Feature Toggles...
		GS.featureToggle = {};
        GS.featureToggle.enableDelete = "{!If($CurrentPage.parameters.enableDelete!=null,$CurrentPage.parameters.enableDelete,false)}";
	</script>


    <div id="gs-app-container">
        <div class="gs-module-container">
	        <div class="data-sync-cntr clearfix"></div>
        </div>
    </div>

    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == null , true, false)}">
            <c:AlertForm />
            <script>
                installResources({scripts: ["datasync/scripts/datasync.bundle.js"], styles:["datasync/styles/datasync.css"]});
            </script>
    </apex:outputpanel>

    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == "dev" , true, false)}">
        <apex:stylesheet value="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath),JSINHTMLENCODE($CurrentPage.parameters.apppath))}/datasync/styles/datasync.css" />
        <script type="text/javascript" src="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath), JSINHTMLENCODE($CurrentPage.parameters.apppath))}/datasync/scripts/datasync.bundle.js"></script>
    </apex:outputpanel>

    <apex:form ></apex:form>

	<script>
		var GSGA = {
			orgId : "{!$Organization.Id}",
			orgName : "{!(JSINHTMLENCODE($Organization.Name))}",
			userName : "{!(JSINHTMLENCODE($User.FirstName))}"+" "+"{!(JSINHTMLENCODE($User.LastName))}"
		};
		(function(i, s, o, g, r, a, m) {
			i['GoogleAnalyticsObject'] = r;
			i[r] = i[r] || function() {
				(i[r].q = i[r].q || []).push(arguments)
			}, i[r].l = 1 * new Date();
			a = s.createElement(o), m = s.getElementsByTagName(o)[0];
			a.async = 1;
			a.src = g;
			m.parentNode.insertBefore(a, m)
		})(window, document, 'script',
				'//www.google-analytics.com/analytics.js', 'ga');
