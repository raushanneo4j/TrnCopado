<apex:page sidebar="false" title="Inbound Email Settings" controller="JBCXM.GSCommonBroker" tabStyle="Administration__tab">
         <div id="gs-app-container">
           <div class="gs-module-container">
              <div class="gainsight-app" style="width:100%">
                <gs-root></gs-root>
               </div>
             </div>
         </div>
    <script src="{!JSINHTMLENCODE(GainsightCDNServerDomain)}gs-ui-static/{!JSINHTMLENCODE(GainsightAppVersion)}/GSVendor/js/vendor/HTMLSanitizer.js"></script>
        <script>
        var hc= fakeEval('{!JSINHTMLENCODE(headerConfig)}');
        var apiEndPoint = hc.gatewayURL;
        var authorization= "";

        var appSessionId = hc.appSessionId;
        var appUserId = hc.appUserId;
        var appOrgId = hc.appOrgId;
        var GS = GS || {};
        GS.commonRemotingEndPoint   = '{!JSINHTMLENCODE($RemoteAction.GSCommonBroker.gsCommonRequestBroker)}';
        GS.gainsightTabs = fakeEval('{!JSINHTMLENCODE(GainsightTabs)}');
        GS.commonRemotingEndPoint   = '{!JSINHTMLENCODE($RemoteAction.GSCommonBroker.gsCommonRequestBroker)}';
        GS.remotingCommonBulkEndPoint  ='{!JSINHTMLENCODE($RemoteAction.GSCommonBroker.gsCommonBulkRequestBroker)}';
        var host = "SALESFORCE";
        GS.appName = "gainsight-ng-ui";
        var urlParams =  window.location.search
            .substring(1)
            .split("&")
            .map(function(v) {
                return v.split("=");
            })
            .reduce(function(acc, arr) {
                if (acc[arr[0]]) {
                    if (!Array.isArray(acc[arr[0]])) {
                        acc[arr[0]] = [acc[arr[0]]]
                    }
                    if(arr[0]) {
                        acc[arr[0]].push(decodeURIComponent(arr[1]))
                    }
                } else {
                    if(arr[0]) {
                        acc[arr[0]] = decodeURIComponent(arr[1]);
                    }
                }
                return acc;
            }, {});
        var app = "user-settings";
    </script>
    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == null , true, false)}">
        <c:Features360 />
        <script>
                installResources({
                                scripts: ["gainsight-ng-ui/" + window["app"] + "/runtime",
                                "gainsight-ng-ui/" + window["app"] + "/polyfills",
                                "gainsight-ng-ui/" + window["app"] + "/main"],
                                styles: ["gainsight-ng-ui/" + window["app"] + "/styles"]
                            });
        </script>
    </apex:outputpanel>
    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == "dev" , true, false)}">
        <apex:stylesheet value="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath),'https://localhost:4200/')}/styles.css" />
        <script>
            GS.cdnUrl = "{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath), "https://localhost:4200/UI")}/";
        </script>
        <script type="text/javascript" src="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath), "https://localhost:4200/UI")}/runtime.js"></script>
        <script type="text/javascript" src="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath), "https://localhost:4200/UI")}/polyfills.js"></script>
        <script type="text/javascript" src="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath), "https://localhost:4200/UI")}/styles.js"></script>
        <script type="text/javascript" src="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath), "https://localhost:4200/UI")}/vendor.js"></script>
        <script type="text/javascript" src="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath), "https://localhost:4200/UI")}/main.js"></script>
    </apex:outputpanel>
</apex:page>