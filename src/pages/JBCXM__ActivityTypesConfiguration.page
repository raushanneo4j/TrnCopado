<apex:page docType="html-5.0" id="ActivityTypeConfig" sidebar="false" title="Activity types configuration | Gainsight" controller="JBCXM.AdminBroker" extensions="JBCXM.NSRemoting,JBCXM.GSCommonBroker" tabStyle="Administration__tab">
    <script src="{!JSINHTMLENCODE(GainsightCDNServerDomain)}gs-ui-static/{!JSINHTMLENCODE(GainsightAppVersion)}/GSVendor/js/vendor/HTMLSanitizer.js"></script>
    <script>
        var nsParams                   = fakeEval('{!JSINHTMLENCODE(nsParams)}');
        var GS                         = GS || {};
        GS.antConfig                   = fakeEval('{!JSINHTMLENCODE(ANTConfig)}');
        var CDNSettings                = fakeEval("{!(JSINHTMLENCODE(GainsightCDNSettings))}");
        GS.remotingConfigEndPoint      = '{!JSINHTMLENCODE($RemoteAction.GSCommonBroker.gsCommonRequestBroker)}';
        GS.remotingCommonBulkEndPoint  ='{!JSINHTMLENCODE($RemoteAction.GSCommonBroker.gsCommonBulkRequestBroker)}';
        GS.commonRemotingEndPoint      = GS.remotingConfigEndPoint;
        GS.adminRemotingEndPoint       = '{!JSINHTMLENCODE($RemoteAction.AdminBroker.adminRequestBroker)}';
        GS.remoting                    = "{!JSINHTMLENCODE($RemoteAction.NSRemoting.apinsinvoke)}";
        GS.NSSessionId                 = "{!(JSINHTMLENCODE(NSSessionId))}";
        GS.orgId                       = "{!JSINHTMLENCODE($Organization.Id)}";
        GS.nsURL                       = nsParams.url;
        GS.activityServiceUrl          = nsParams.url + "/ant";
        GS.isRelationshipEnabled       = {!(isRelationshipEnabled)};
        GS.userConfig = {
                        userLocale  : "{!(JSINHTMLENCODE(dateTimeFormatByLocale))}",
                        userName    : "{!(JSINHTMLENCODE($User.FirstName))}"+" "+"{!(JSINHTMLENCODE($User.LastName))}",
                        firstName    : "{!(JSINHTMLENCODE($User.FirstName))}",
                        lastName    : "{!(JSINHTMLENCODE($User.LastName))}",
                        userId      : "{!JSINHTMLENCODE($User.Id)}",
                        userDisplayName : "{!(JSINHTMLENCODE(userDisplayName))}",
                        profilePhoto    : "{!(JSINHTMLENCODE(profilePhoto))}",
                        userEmail   : "{!JSINHTMLENCODE($User.Email)}",
                        userSessionId : "{!(JSINHTMLENCODE(sessionId))}"
                    };
    </script>

    <apex:outputPanel style="display:none;">
        <apex:form >
            <input type="date"/>
        </apex:form>
    </apex:outputPanel>

    <div id="gs-app-container">
        <div class="gs-activity-main-container"></div>
    </div>
    <script src="{!JSINHTMLENCODE(GainsightCDNServerDomain)}gs-ui-static/{!JSINHTMLENCODE(GainsightAppVersion)}/GSApp/cdnVersionResolver.js"  type="text/javascript"></script>

    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == null , true, false)}">
        <c:AlertForm />
        <script>
            installResources({scripts: ["activities_timeline_admin/scripts/activities_timeline_admin.bundle.js"], styles:["activities_timeline_admin/styles/activities_timeline_admin.css"]});
        </script>
    </apex:outputpanel>

    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == "dev" , true, false)}">
        <apex:stylesheet value="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath),'https://localhost:8080/UI')}/activities_timeline_admin/styles/activities_timeline_admin.css" />
        <script type="text/javascript" src="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath), "https://localhost:8080/UI")}/activities_timeline_admin/scripts/activities_timeline_admin.bundle.js"></script>
    </apex:outputpanel>

    <script>
        var GSGA = {
            orgId   : "{!JSINHTMLENCODE($Organization.Id)}",
            orgName : "{!(JSINHTMLENCODE($Organization.Name))}",
            userName: "{!(JSINHTMLENCODE($User.FirstName))}"+" "+"{!(JSINHTMLENCODE($User.LastName))}"
        };
        (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
                    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
                m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
        })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
    </script>
    <script src="{!JSINHTMLENCODE(GainsightCDNServerDomain)}gs-ui-static/{!JSINHTMLENCODE(GainsightAppVersion)}/CustomJS/CustomJS/analyticsWrapper.js"></script>
    <script>
        gainsightTrackEvents.pushPageViews('ActivityTypeConfig');
    </script>
    <apex:Form ></apex:Form>
</apex:page>