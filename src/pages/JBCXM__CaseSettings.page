<apex:page docType="html-5.0" sidebar="false" title="C360 Layouts Configuration | Gainsight" controller="JBCXM.ConfigBroker" extensions="JBCXM.NSRemoting,JBCXM.GSCommonBroker" tabStyle="Administration__tab">
    <script>
        var GS                         = GS || {};
        GS.remotingConfigEndPoint      = '{!$RemoteAction.GSCommonBroker.gsCommonRequestBroker}';
        GS.remotingCommonBulkEndPoint  ='{!$RemoteAction.GSCommonBroker.gsCommonBulkRequestBroker}';
        GS.remotingEndPoint            = '{!$RemoteAction.ConfigBroker.configRequestBroker}';
        GS.commonRemotingEndPoint      = GS.remotingConfigEndPoint;
        GS.remoting                    = "{!$RemoteAction.NSRemoting.apinsinvoke}";
        GS.NSSessionId                 = "{!(NSSessionId)}";
        GS.orgId                       = "{!$Organization.Id}";
        GS.packageNS                   = "{!(pkgNS)}";
        GS.isPackagedInstance          = (GS.packageNS) ? "true" : "false";
    </script>
 
    <div id="gs-app-container">
        <div class="gs-module-container">
            <div class="gs-cs360layouts-main-container"></div>
        </div>
    </div>
    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == null , true, false)}">
      <c:AlertForm />
      <script>
        installResources({scripts: ["cs360Layouts/scripts/cs360Layouts.bundle.js"], styles:["cs360Layouts/styles/cs360Layouts.css"]});
      </script>
    </apex:outputpanel>
    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == "dev" , true, false)}">
        <apex:stylesheet value="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath),'https://localhost:9091/UI')}/cs360Layouts/styles/cs360Layouts.css" />
        <script type="text/javascript" src="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath), "https://localhost:9091/UI")}/cs360Layouts/scripts/cs360Layouts.bundle.js"></script>
    </apex:outputpanel>
       <script>
        var GSGA = {
            orgId   : "{!$Organization.Id}",
            orgName : "{!(JSINHTMLENCODE($Organization.Name))}",
            userName: "{!(JSINHTMLENCODE($User.FirstName))}"+" "+"{!(JSINHTMLENCODE($User.LastName))}"
        };
