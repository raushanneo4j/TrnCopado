<apex:page sidebar="false" title="Merge Companies" controller="JBCXM.AdminBroker" tabStyle="Administration__tab" extensions="JBCXM.GSCommonBroker">
         <div id="gs-app-container">
           <div class="gs-module-container">
              <div class="gainsight-app" style="width:100%">
                <gs-root></gs-root>
               </div>
             </div>
         </div>

           <style>
        .bPageFooter.noTableFooter{
            display:none;
        }
    </style>
    <script src="{!JSINHTMLENCODE(GainsightCDNServerDomain)}gs-ui-static/{!JSINHTMLENCODE(GainsightAppVersion)}/GSVendor/js/vendor/HTMLSanitizer.js"></script>
        <script>
            var hc= fakeEval('{!JSINHTMLENCODE(headerConfig)}');
            var apiEndPoint = hc.gatewayURL;
            var authorization= "";
			
            var appSessionId = hc.appSessionId;
            var appUserId = hc.appUserId;
            var appOrgId = hc.appOrgId;
            var GS = GS || {};
            GS.commonRemotingEndPoint   = '{!$RemoteAction.GSCommonBroker.gsCommonRequestBroker}';
            GS.gainsightTabs = fakeEval('{!JSINHTMLENCODE(GainsightTabs)}');

            GS.commonRemotingEndPoint   = '{!$RemoteAction.GSCommonBroker.gsCommonRequestBroker}';
            GS.remotingCommonBulkEndPoint  ='{!$RemoteAction.GSCommonBroker.gsCommonBulkRequestBroker}';
    		GS.userConfig = {
                                userLocale          : {!(dateTimeFormatByLocale)},
                                userName            : "{!(JSINHTMLENCODE($User.FirstName))}"+" "+"{!(JSINHTMLENCODE($User.LastName))}",
                                userId              : "{!$User.Id}",
                                emailId             : "{!$User.Email}",
                                profilePhoto        : "{!(profilePhoto)}",
                                userDisplayName     : "{!(JSINHTMLENCODE(userDisplayName))}",
                                gainsightTabs       : fakeEval('{!JSINHTMLENCODE(gainsightTabs)}'),
                                isLightningEnabled  : {!(isLightningEnabled)}
                            };      
        	GS.currencyConfig = fakeEval('{!JSINHTMLENCODE(currencyConfigJSON)}');
            var host = "SALESFORCE";
    		GS.appName = "gainsight-ng-ui/objects/company-admin";

            var urlParams =  window.location.search
                .substring(1)
                .split("&")
                .map(function(v) {
                    return v.split("=");
                })
                .reduce(function(acc, arr) {
                    if (acc[arr[0]]) {
                        if (!Array.isArray(acc[arr[0]])) {
                            acc[arr[0]] = [acc[arr[0]]]
                        }
                        if(arr[0]) {
                            acc[arr[0]].push(decodeURIComponent(arr[1]))
                        }
                    } else {
                        if(arr[0]) {
                            acc[arr[0]] = decodeURIComponent(arr[1]);
                        }
                    }
                    return acc;
                }, {});

            
        </script>
    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == null , true, false)}">
        <c:Features360 />
        <script>
        		GS.workspace = "company-admin";
                document.querySelector(".gs-module-container").innerHTML = '<div class="gainsight-app" style="width:100%"><gs-root></gs-root></div>';
                installResources({
                    scripts: [
                        "runtime-es5.js",
                        "runtime-es2015.js",
                        "polyfills-es2015.js",
                        "polyfills-es5.js",
                        "main-es2015.js",
                        "main-es5.js"
                    ],
                    styles: ["styles.css"]
                });
        </script>
    </apex:outputpanel>
        <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == "dev" , true, false)}">
            <apex:stylesheet value="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath),'http://localhost:4200/')}/styles.css" />
        <script>
            GS.cdnUrl ="http://localhost:4200/";
        </script>
        <apex:stylesheet value="https://fonts.googleapis.com/icon?family=Material+Icons"/>
        <script type="text/javascript" src="http://localhost:4200/runtime.js"></script>
        <script type="text/javascript" src="http://localhost:4200/polyfills.js"></script>
        <script type="text/javascript" src="http://localhost:4200/styles.js"></script>
        <script type="text/javascript" src="http://localhost:4200/vendor.js"></script>
        <script type="text/javascript" src="http://localhost:4200/main.js"></script>
</apex:outputpanel>


        </apex:page>