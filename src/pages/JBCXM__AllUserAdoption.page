<apex:page docType="html-5.0" controller="JBCXM.LayoutBroker" sidebar="false" title="Engagement" extensions="JBCXM.NSRemoting,JBCXM.GSCommonBroker"> 
	<div id="gs-app-container">
	 <div class="gs-module-container">
		 <div class="container-fluid adoption_ctn">	      
	    </div>
	 </div>
	</div>
     <style>
         .node {
            border:solid 1px #FFF;
            font-size:11px;
            font-weight:700;
            line-height:12px;
            overflow:hidden;
            position:absolute;
            text-indent:2px;
        }
     </style>
    <script src="{!JSINHTMLENCODE(GainsightCDNServerDomain)}gs-ui-static/{!JSINHTMLENCODE(GainsightAppVersion)}/GSVendor/js/vendor/HTMLSanitizer.js"></script>
    <script>
        var nsParams  = fakeEval('{!JSINHTMLENCODE(nsParams)}');
        var GS = GS || {};
            GS.userConfig = {
                userLocale  : {!(dateTimeFormatByLocale)},
                userName    : "{!(JSINHTMLENCODE($User.FirstName))}"+" "+"{!(JSINHTMLENCODE($User.LastName))}",
                userId      : "{!$User.Id}",
                emailId     : "{!$User.Email}",
                gainsightTabs: fakeEval('{!JSINHTMLENCODE(gainsightTabs)}'),
                isLightningEnabled: {!(isLightningEnabled)}
            };
            GS.remoting					= "{!$RemoteAction.NSRemoting.apinsinvoke}";
            GS.remotingEndPoint        	= '{!$RemoteAction.LayoutBroker.layoutRequestBroker}';
            GS.remotingBulkEndPoint    	= '{!$RemoteAction.LayoutBroker.layoutBulkRequestBroker}';
            GS.commonRemotingEndPoint	= '{!$RemoteAction.GSCommonBroker.gsCommonRequestBroker}';
            
            GS.customLabels         = fakeEval('{!JSINHTMLENCODE(customLabelsMap)}');
            GS.operatorsListMap     = fakeEval('{!JSINHTMLENCODE(operatorsListMap)}');
            GS.fiscalYrEnabled      = {!(IsCustomFiscalYearSettingsEnabled)};
            GS.customLookUpConfig   = JSON.parse('{!(JSENCODE(customLookUpConfigMap))}');
            GS.churnLabelMap        = fakeEval('{!JSINHTMLENCODE(churnLabelMap)}');
            GS.currencyConfig       = fakeEval('{!JSINHTMLENCODE(currencyConfigJSON)}');
            
            GS.profilePhoto        =  "{!(profilePhoto)}";
            GS.userDisplayName       = "{!(JSINHTMLENCODE(userDisplayName))}";
            
            GS.packageNS            = "{!(pkgNS)}";
            GS.isPackagedInstance   = (GS.packageNS) ? "true" : "false";
            
            GS.generalPageSize 	= parseInt('{!(UsageDataRecordLimit)}');
            GS.scmuPageSize 	= 100;
            GS.useSparkLinesforAdoption = true;
                        
            GS.sessionId 	= "{!NSSessionId}";
            GS.remoting 	= "{!$RemoteAction.NSRemoting.apinsinvoke}";
            
            GS.ns			= {allowed:"false"};         
            
            GS.ns.allowed = GS.ns.allowed == "true" || GS.ns.allowed == true;
            GS.statePreservation 	= {}; 
            GS.isEngTrends = true;
    </script>
    
    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == null , true, false)}">
        <c:AlertForm />
        <script>
            installResources({scripts: ["adoptionAnalytics/scripts/adoptionAnalytics.bundle.js"], styles:["adoptionAnalytics/styles/adoptionAnalytics.css"]});
        </script>
    </apex:outputpanel>
    <apex:outputpanel rendered="{!If(JSINHTMLENCODE($CurrentPage.parameters.env) == "dev" , true, false)}">
        <apex:stylesheet value="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath),'https://localhost:8080/UI')}/adoptionAnalytics/styles/adoptionAnalytics.css" />
        <script type="text/javascript" src="{!IF(JSINHTMLENCODE($CurrentPage.parameters.apppath)!=null,JSINHTMLENCODE($CurrentPage.parameters.apppath), "https://localhost:8080/UI")}/adoptionAnalytics/scripts/adoptionAnalytics.bundle.js"></script>
    </apex:outputpanel>
    
    <script>
    	var GSGA = {
			orgId: "{!$Organization.Id}",
			orgName: "{!(JSINHTMLENCODE($Organization.Name))}",
			userName: "{!(JSINHTMLENCODE($User.FirstName))}"+" "+"{!(JSINHTMLENCODE($User.LastName))}"
		};
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		})(window,document,'script','//www.google-analytics.com/analytics.js','ga');	
	</script>
	<script src="{!JSINHTMLENCODE(GainsightCDNServerDomain)}gs-ui-static/{!JSINHTMLENCODE(GainsightAppVersion)}/CustomJS/CustomJS/analyticsWrapper.js"></script>
	
	<script>
        gainsightTrackEvents.pushPageViews('All User Adoption');
    </script>
    <apex:form ></apex:form>
</apex:page>