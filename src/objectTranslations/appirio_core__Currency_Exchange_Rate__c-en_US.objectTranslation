        <label><!-- Effective Date --></label>
        <name>appirio_core__Effective_Date__c</name>
    </fields>
    <fields>
        <help><!-- Specifies the date on which this Exchange Rate expires, if any.  The Exchange Rate is valid until the day before the Expiration Date, or never expires if blank. --></help>
        <label><!-- Expiration Date --></label>
        <name>appirio_core__Expiration_Date__c</name>
    </fields>
    <fields>
