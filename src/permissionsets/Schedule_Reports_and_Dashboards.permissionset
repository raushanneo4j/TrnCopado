<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>This permission set combines two separate permissions -- Schedule Reports and Schedule Dashboards. The two go hand-in-hand. 

JIRA: SFDCSPT-103; 8/10/2020</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Schedule Reports &amp; Dashboards</label>
    <userPermissions>
        <enabled>true</enabled>
        <name>RunReports</name>
    </userPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>ScheduleJob</name>
    </userPermissions>
    <userPermissions>
        <enabled>true</enabled>
        <name>ScheduleReports</name>
    </userPermissions>
</PermissionSet>