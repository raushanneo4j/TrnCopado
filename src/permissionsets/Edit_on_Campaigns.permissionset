<?xml version="1.0" encoding="UTF-8"?>
<PermissionSet xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Edit level of Access to the Campaign object. Example use case: a user would need this access in order to add Leads or Contacts to a Campaign. JIRA: SFDCSPT-86</description>
    <hasActivationRequired>false</hasActivationRequired>
    <label>Edit on Campaigns</label>
    <objectPermissions>
        <allowCreate>false</allowCreate>
        <allowDelete>false</allowDelete>
        <allowEdit>true</allowEdit>
        <allowRead>true</allowRead>
        <modifyAllRecords>false</modifyAllRecords>
        <object>Campaign</object>
        <viewAllRecords>false</viewAllRecords>
    </objectPermissions>
</PermissionSet>